<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.megaestruc.megaestruc_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".vista.ClienteView.NuevoCliente"></activity>
        <activity android:name=".vista.CotizacionView.ListaCotizacionProducto" />
        <activity android:name=".vista.ProductoView.DetalleProducto" />
        <activity android:name=".vista.ProductoView.ListaProductos" />
        <activity android:name=".vista.ClienteView.ListaCliente" />
        <activity android:name=".vista.CotizacionView.Producto" />
        <activity android:name=".vista.CotizacionView.Cotizacion" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>